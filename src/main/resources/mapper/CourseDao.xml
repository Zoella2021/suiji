<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Dao.CourseDao">
    <resultMap id="CourseMap" type="com.example.demo.Model.Course">
        <id column="courseid" jdbcType="BIGINT" property="courseid"/>
        <result column="coursename" jdbcType="VARCHAR" property="coursename"/>
        <result column="classroomid" jdbcType="VARCHAR" property="classroomid"/>
        <result column="teachername" jdbcType="VARCHAR" property="teachername"/>
        <result column="time" jdbcType="VARCHAR" property="time"/>
    </resultMap>

    <select id="selectByCourse_name" parameterType="string" resultMap="CourseMap">
        select * FROM course c where c.coursename like "%"#{coursename}"%"
    </select>

    <delete id="deleteByCourseId" parameterType="long">
        DELETE FROM course where courseid=#{coursseid}
    </delete>
    <insert id="insertCourse" parameterType="Course">
        INSERT INTO course (courseid,coursename, classroomid, teachername,time) Value(#{courseid},#{coursename},#{classroomid},#{teachername},#{time})
    </insert>

</mapper>